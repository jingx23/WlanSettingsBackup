<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#FFFFFF"
    >

<Button android:id="@+id/Button01" android:layout_height="wrap_content" android:text="@string/btnNameBackup" android:onClick="@string/btnClickHandlerBackup" android:layout_width="fill_parent" android:layout_marginTop="5px"></Button>


<Button android:id="@+id/Button02" android:text="@string/btnNameRestore" android:onClick="@string/btnClickHandlerRestore" android:layout_height="wrap_content" android:layout_width="fill_parent"></Button>
<TextView android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="@string/infoMessage" android:id="@+id/TextView01" android:textColor="#000000"></TextView>
</LinearLayout>
